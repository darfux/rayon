<% @projects.each do |project|%>
<tr>
    <td><%= project.id %></td>
    <td><%= project.name %></td>
    <td><%= get_brief(project) %></td>
    <td><%= project.start.to_s+'/'+project.end.to_s %></td>
    <td><%= get_state(project) %></td>
    <td><%= get_source(project) %></td>
    <td><%= get_participation(project, @user) %></td>
    <td>
        <%= link_to 'show', project, remote: true %>
        <%= link_to 'edit', edit_project_path(project.id), remote: true %>
        <a>delete</a>
    </td>
</tr>
<% end %>